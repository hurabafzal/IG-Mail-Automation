model combined_daily_statistics_recovery {
  stat_date                 DateTime @id @db.Date
  valid                     Int      @default(0)
  invalid                   Int      @default(0)
  account_disabled          Int      @default(0)
  action_required           Int      @default(0)
  captcha                   Int      @default(0)
  captcha_disabled          Int      @default(0)
  compromised               Int      @default(0)
  email_confirmation        Int      @default(0)
  email_verification        Int      @default(0)
  invalid_credentials       Int      @default(0)
  password_reset            Int      @default(0)
  pending                   Int      @default(0)
  phone_validation          Int      @default(0)
  scrape_warning            Int      @default(0)
  suspended                 Int      @default(0)
  temporary_locked          Int      @default(0)
  delayed                   Int      @default(0)
  email_connection_error    Int      @default(0)
  initializing              Int      @default(0)
  stopped                   Int      @default(0)
  two_factor_authentication Int      @default(0)
  created_at                DateTime @default(now()) @db.Timestamptz(6)
}

